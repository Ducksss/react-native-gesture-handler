{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "react-native-gesture-handler": [
        "./index.ts"
      ],
      "react-native-gesture-handler/Swipeable": ["./src/components/Swipeable.tsx"],
      "react-native-gesture-handler/DrawerLayout": ["./src/components/DrawerLayout.tsx"]
    },
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext",
    "allowJs": true,
    "declaration": true,
    "declarationMap": true,
    "noStrictGenericChecks": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitUseStrict": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,

    "outDir": "./dist"
  },
  "include": ["*.ts", "src/**/*.ts", "src/**/*.tsx", "examples/Example/src/**/*.ts", "examples/Example/src/**/*.tsx", "examples/Example/index.js"]
}
